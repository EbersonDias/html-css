Algoritmo "Somador Numerico"

Var
   cont, N, S : Inteiro
Inicio
   cont <- 1
   S <- 0
   Enquanto (cont <=10) faca
        Escreva ("Digite o", cont,"o. um Valor: ")
        Leia (N)
        S <- S + N
        cont <- cont + 1
   FimEnquanto
   Escreva (" A Soma de Todos os Valores Foi ", S)
Fimalgoritmo